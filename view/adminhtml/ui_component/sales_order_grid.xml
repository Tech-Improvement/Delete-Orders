<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="delete">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">delete-orders</item>
                        <item name="label" xsi:type="string" translate="true">Delete Order(s)</item>
                        <item name="url" xsi:type="url" path="techimprovement_deleteorders/delete/massdeleteorders"/>
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Delete selected items</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure you want to delete the
                                selected orders?
                            </item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
